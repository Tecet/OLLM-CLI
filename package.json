{
  "name": "ollm-cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "echo 'Dev mode not yet implemented'",
    "start": "node packages/cli/dist/cli.js",
    "test": "vitest run",
    "test:all": "vitest run",
    "test:unit": "vitest run",
    "//validate-docs": "DISABLED: node scripts/validate-docs.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preversion": "npm run test && npm run lint",
    "version": "node scripts/version-bump.js && git add -A",
    "postversion": "git push && git push --tags",
    "verify-release": "node scripts/verify-release.js",
    "task:start": "node scripts/task-tracker.js start",
    "task:complete": "node scripts/task-tracker.js complete",
    "task:credits": "node scripts/task-tracker.js credits",
    "task:status": "node scripts/task-tracker.js status",
    "task:sync": "node scripts/task-tracker.js sync",
    "generate-model-db": "node scripts/generate_model_db.js",
    "validate-profiles": "node scripts/validate_profiles.js",
    "validate:prompts": "node scripts/validate-promptBudget.js",
    "prebuild": "npm run generate-model-db"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "ajv": "^8.12.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-check": "^3.15.0",
    "globals": "^17.0.0",
    "ink": "^6.6.0",
    "ink-testing-library": "github:vadimdemedes/ink-testing-library",
    "jsdom": "^27.4.0",
    "prettier": "^3.0.0",
    "react": "^19.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@xterm/headless": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "jimp": "^1.6.0",
    "node-pty": "^1.1.0",
    "sharp": "^0.34.5",
    "strip-ansi": "^7.1.2",
    "terminal-image": "^1.1.0",
    "tiktoken": "^1.0.22"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-reconciler": "^0.33.0"
  }
}
